{
    "menu": "rofi -dmenu -p 'quickterm: ' -no-custom -auto-select",
    "term": "foot",
    "history": "{$HOME}/.cache/i3/i3-quickterm.order",
    "ratio": 0.55,
    "pos": "center",
    "shells": {
        "shell": "{$SHELL}",
        "btm_simple": "{$SHELL} -c 'btm -b'",
        "battery": "{$SHELL} -c 'battop'",
        "weather": "{$SHELL} -c 'curl \"wttr.in/?1n\"'"

    }
}
